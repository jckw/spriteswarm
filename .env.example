# Sprite Orchestrator Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8080

# Sprites.dev API
SPRITES_TOKEN=your-sprites-api-token

# Admin API Authentication
ADMIN_TOKEN=your-admin-secret-token

# GitHub Webhook (HMAC secret configured in GitHub webhook settings)
GITHUB_WEBHOOK_SECRET=your-github-webhook-secret

# Cloudflare KV Storage
CF_ACCOUNT_ID=your-cloudflare-account-id
CF_API_TOKEN=your-cloudflare-api-token
CF_KV_NAMESPACE_ID=your-kv-namespace-id
